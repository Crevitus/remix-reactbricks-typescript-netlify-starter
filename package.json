{
    "private": true,
    "name": "personal-site",
    "description": "",
    "license": "",
    "scripts": {
        "build": "run-s build:*",
        "build:css": "npm run generate:css -- --minify",
        "build:remix": "remix build",
        "dev": "run-p dev:*",
        "dev:css": "npm run generate:css -- --watch",
        "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
        "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
        "test": "vitest",
        "start": "cross-env NODE_ENV=production remix-serve build",
        "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
        "start-local": "node -r dotenv/config node_modules/.bin/remix-serve build",
        "postinstall": "remix setup netlify"
    },
    "dependencies": {
        "@netlify/functions": "^1.0.0",
        "@remix-run/netlify": "^1.3.3",
        "@remix-run/react": "^1.3.3",
        "@remix-run/serve": "^1.3.3",
        "dotenv": "^16.0.0",
        "react": "^18.0.0",
        "react-bricks": "^3.2.16",
        "react-bricks-ui": "^0.3.20",
        "react-dom": "^18.0.0",
        "leaflet": "^1.7.1",
        "remix": "^1.3.3",
        "prism-theme-night-owl": "^1.4.0",
        "prismjs": "^1.27.0"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.3.3",
        "@remix-run/eslint-config": "^1.3.3",
        "@testing-library/jest-dom": "^5.16.3",
        "@testing-library/react": "^12.1.4",
        "@testing-library/user-event": "^14.0.0",
        "@testing-library/dom": "^8.12.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/eslint": "^8.4.1",
        "@types/react": "^17.0.43",
        "@types/react-dom": "^17.0.14",
        "@types/node": "^17.0.23",
        "@vitejs/plugin-react": "^1.2.0",
        "autoprefixer": "^10.4.4",
        "binode": "^1.0.5",
        "cross-env": "^7.0.3",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "happy-dom": "^2.51.0",
        "msw": "^0.39.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.8",
        "prettier": "2.6.1",
        "prettier-plugin-tailwindcss": "^0.1.8",
        "tailwindcss": "^3.0.23",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.14.1",
        "typescript": "^4.6.3",
        "vite-tsconfig-paths": "^3.4.1",
        "vite": "^2.9.0",
        "vitest": "^0.8.0"
    },
    "engines": {
        "node": ">=16"
    },
    "sideEffects": false,
    "author": "Ben Crevis",
    "volta": {
        "node": "16.14.2",
        "yarn": "1.22.18"
    }
}
